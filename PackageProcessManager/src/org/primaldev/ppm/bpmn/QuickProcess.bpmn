    </userTask>
    <sequenceFlow id="flow3" sourceRef="getInfo" targetRef="GetMoreInfo"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="GetMoreInfo" targetRef="endevent1"></sequenceFlow>
