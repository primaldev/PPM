        <omgdi:waypoint x="452.0" y="340.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="452.0" y="340.0"></omgdi:waypoint>
        <omgdi:waypoint x="637.0" y="285.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
